xfconf-query --channel xfce4-keyboard-shortcuts --property "/commands/custom/<Super>h" --create --type string --set "wmctrl -s 9 "
xfconf-query --channel xfce4-keyboard-shortcuts --property "/commands/custom/<Super>x" --create --type string --set "wmctrl -s 10"
xfconf-query --channel xfce4-keyboard-shortcuts --property "/commands/custom/<Super>c" --create --type string --set "wmctrl -s 11"
xfconf-query --channel xfce4-keyboard-shortcuts --property "/commands/custom/<Super>v" --create --type string --set "wmctrl -s 12"
xfconf-query --channel xfce4-keyboard-shortcuts --property "/commands/custom/<Super>b" --create --type string --set "wmctrl -s 13"
xfconf-query --channel xfce4-keyboard-shortcuts --property "/commands/custom/<Super>n" --create --type string --set "wmctrl -s 14"


